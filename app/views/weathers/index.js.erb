$('#results').empty(); // clears results div before adding new lyrics
$("#results").append("<%= j render(partial: 'results') %>"); // add partial _results.html.erb



// show umbrella on if precipitaion is over 40%

let pop = $('#pop').text().split(" ")[3];
let popInt = parseInt(pop);
const umbrella = function(popInt) {
  if (popInt < 40){
    $('#umbrella_image').css('display', 'none');
  }
}
umbrella(popInt);

